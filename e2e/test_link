#!/usr/bin/env bash
set -euo pipefail
# shellcheck source-path=SCRIPTDIR
source "$(dirname "$0")/assert.sh"

mise plugins ln "$ROOT"/test/data/plugins/tiny
assert_contains "mise plugins" "tiny"

assert_fail "mise plugins link $ROOT/test/data/plugins/tiny"
mise plugins link -f "$ROOT"/test/data/plugins/tiny
assert_contains "mise plugins" "tiny"

mise plugins rm tiny
assert_not_contains "mise plugins" "tiny"
