#!/usr/bin/env bash
set -euo pipefail
# shellcheck source-path=SCRIPTDIR
source "$(dirname "${BASH_SOURCE[0]}")"/../assert.sh

cat <<EOF >.mise.toml
[tools]
tiny = "latest"
EOF

mise i tiny
assert_contains "mise env -s bash" "tiny/latest"

mise use tiny@system
assert_not_contains "mise env -s bash" "tiny/latest"
